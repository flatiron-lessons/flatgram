	<h1> Flatiron Gram </h1>
<fieldset>
	<legend> We value our users' privacy. Please sign in to start checking out some content! </legend>
	<%= form_tag(sessions_path) do %>
		Email: <%= text_field_tag :email, nil, required: true %>
		Password: <%= password_field_tag :password, nil, required: true %>
		<%= submit_tag 'Login!' %>
	<% end %>
</fieldset>
